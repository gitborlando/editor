      "export const SpaceBetweenItem = forwardRef<",
      "  HTMLDivElement,",
      "  ComponentPropsWithRef<'div'> & {}",
      ">(({ className, ...rest }, ref) => {",
      "  return <G className={cls()} {...rest} ref={ref}></G>",
      "})",
